
public class ComputePrimes 
{

	public static void main(String[] args) 
	{
		//instantiate a new class to enable access to all methods of the class
		ComputePrimes cp = new ComputePrimes();

		int sz = 25;

		int primes[] = new int[sz];

		int i = 0;
		int num = 2;

//		for(int num = 2; i < sz; num++)
		
		while(i <sz)
		{
			if (cp.isprime(num))
			{
				primes[i] = num;
				i++;
			}
			num++;
		}
		
		//print primes array
		for (i =0; i<sz; i++)
		{
			System.out.println(" " + primes[i]);
		}

	}


	public boolean isprime (int n)
	{
		//isprime checks whether the value is prime and returns true or false accordingly

		for (int i = 2; i <= Math.sqrt(n); i++)
		{
			if (n % i == 0)
				return false;	
		}		

		return true;
	}

}
